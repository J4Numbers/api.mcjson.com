{
  "name": "mcjson.com",
  "version": "0.0.1",
  "description": "REST/JSON server providing minecraft data in a consumable format.",
  "main": "index.js",
  "scripts": {
    "app:prod:start": "pm2 start process.json --env production",
    "app:prod:start:local": "pm2 start process.json --env production --no-daemon",
    "app:prod:stop": "pm2 stop process.json",
    
    "app:dev:start": "pm2 start process.json --env development --no-daemon",

    "data:git:init": "git clone https://github.com/tehbeard/api.mcjson.com-data.git data",
    "data:git:pull": "cd data && git pull && cd .. && pm2 restart process.json",
    "data:git:commit": "cd data && git add . && git status --porcelain >> ../ver-commit.txt && git commit -t ../ver-commit.txt && rm ../ver-commit.txt && cd ../",
    "data:git:push": "cd data && git push && cd ../",
    
    "data:fetch:versions": "node scripts/fetchVersions.js",
    "data:fetch:graham": "node scripts/fetchFromGraham.js"
  },
  "author": "Tehbeard",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "html-webpack-plugin": "^2.26.0",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.1",
    "react-select": "^1.0.0-beta11",
    "gql-loader": "github:tehbeard/gql-loader"
  },
  "dependencies": {
    "boolean": "^0.1.1",
    "co": "^4.6.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-graphql": "^0.6.2",
    "graphql": "^0.9.0",
    "graphql-tools": "^0.9.2",
    "inquirer": "^3.0.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.3",
    "readdir-plus": "^1.0.0",
    "response-time": "^2.3.2",
    "semver": "^5.3.0"
  }
}
