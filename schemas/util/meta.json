{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "key": {
            "type": "string"
        },
        "values": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "mask": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 15
                    },
                    "value": {
                        "type": "string"
                    }
                },
                "required": [
                    "mask",
                    "value"
                ]
            }
        }
    },
    "required": [
        "key",
        "values"
    ]
}