{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title":"Metadata",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "key": {
                "title": "Variant",
                "type": "string"
            },
            "values": {
                "type": "array",
                "title":"Values",
                "items": {
                    "type": "object",
                    "title":"Value pair",
                    "properties": {
                        "mask": {
                            "title":"Bitmask/raw value",
                            "type": "integer",
                            "minimum": 0
                        },
                        "value": {
                            "title":"Label",
                            "type": "string"
                        }
                    },
                    "required": [
                        "mask",
                        "value"
                    ]
                }
            }
        },
        "required": [
            "key",
            "values"
        ]
    }
}