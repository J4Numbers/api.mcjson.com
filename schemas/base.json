{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "mod": {
            "description": "Mod id",
            "type": "string"
        },
        "id": {
            "description": "id",
            "type": "string"
        },
        "name": {
            "description": "Human name",
            "type": "string"
        },
        "introduced_at": {
            "description": "Version introduced to game",
